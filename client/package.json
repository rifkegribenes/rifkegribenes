{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "autoprefixer": "^9.4.3",
    "chokidar-cli": "^1.2.1",
    "immutability-helper": "^2.8.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.1.0",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-api-middleware": "^2.3.0",
    "smoothscroll-polyfill": "^0.4.3"
  },
  "devDependencies": {
    "react-scripts": "2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-task:scss-compile": "node-sass-chokidar --source-map true src/styles/scss/ -o public/css",
    "build-task:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/styles/scss/' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
